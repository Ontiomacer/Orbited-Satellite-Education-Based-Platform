# Worqhat AI API Configuration
# Get your API key from: https://worqhat.com/
VITE_WORQHAT_API_KEY=your_worqhat_api_key_here

# Application Configuration
VITE_APP_NAME=Satellite Tracking Platform
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=https://api.worqhat.com

# Development Configuration
VITE_DEV_MODE=true
VITE_LOG_LEVEL=info

# Optional: Custom workflow configuration
VITE_DEFAULT_WORKFLOW_ID=your_default_workflow_id_here

# Optional: Satellite tracking configuration
VITE_DEFAULT_SATELLITE_NORAD_ID=25544
VITE_SATELLITE_UPDATE_INTERVAL=5000

# Optional: UI Configuration
VITE_THEME=dark
VITE_ENABLE_ANIMATIONS=true